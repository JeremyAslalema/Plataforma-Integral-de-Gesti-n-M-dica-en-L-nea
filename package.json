{
  "name": "plataforma-medica",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "set PRISMA_CLI_BINARY_TARGETS=debian-openssl-3.0.x,rhel-openssl-3.0.x&& npx prisma generate&& next build&& node scripts/post-build.js",
    "start": "node .next/standalone/server.js",
    "lint": "eslint",
    "postinstall": "set PRISMA_CLI_BINARY_TARGETS=debian-openssl-3.0.x,rhel-openssl-3.0.x&& npx prisma generate",
    "vercel-build": "PRISMA_CLI_BINARY_TARGETS=debian-openssl-3.0.x,rhel-openssl-3.0.x npx prisma generate && next build && node scripts/post-build.js",
    "db:push": "npx prisma db push",
    "fix-prisma": "node scripts/post-build.js"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.11.1",
    "@prisma/client": "5.22.0",
    "bcryptjs": "^3.0.3",
    "next": "16.0.0",
    "next-auth": "^4.24.13",
    "prisma": "5.22.0",
    "react": "19.2.0",
    "react-dom": "19.2.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.0.0",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
